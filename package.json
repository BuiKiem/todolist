{
  "name": "todolist",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.3.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/material-ui": "^0.21.7",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.3.1",
    "react-scripts": "3.4.1",
    "typeface-roboto": "^0.0.75",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "build:tailwindcss": "postcss src/tailwind.css -o src/styles.css",
    "prestart": "yarn build:tailwindcss",
    "start": "react-scripts start",
    "prebuild": "NODE_ENV=production yarn build:tailwindcss",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "format": "prettier --write \"src/**/*.{tsx,ts,css}\"",
    "eject": "react-scripts eject",
    "storybook": "yarn build:tailwindcss && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "autoprefixer": "^9.7.5",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.2",
    "serve": "^11.3.0",
    "tailwindcss": "^1.2.0"
  }
}
